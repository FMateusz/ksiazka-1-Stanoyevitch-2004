\newenvironment{Example}[1][]{\refstepcounter{Example}\par\medskip
\noindent\textbf{EXAMPLE~\thechapter.\theExample: #1 \label{example:\thechapter_\theExample}} \rmfamily}{\medskip}

\newenvironment{ExerciseForTheReader}[1][]{\refstepcounter{ExerciseForTheReader}\par\medskip
\noindent\textnormal{EXERCISE FOR THE READER~\thechapter.\theExerciseForTheReader: #1 \label{exreader:\thechapter_\theExerciseForTheReader}} \rmfamily}{\medskip}

\newenvironment{Program}[1][]{\refstepcounter{Program}\par\medskip
\noindent\textbf{PROGRAM~\thechapter.\theProgram: #1 \label{program:\thechapter_\theProgram}} \rmfamily}{\medskip}

\newenvironment{Exercises}[1][]{\refstepcounter{Exercises}\par\medskip
\noindent\textbf{\noindent\rule{\textwidth}{1pt} EXERCISES~\thechapter.\theExercises:\\ #1 \label{exercises:\thechapter_\theExercises}} \rmfamily}{\medskip}

\newenvironment{Table}[1][]{\refstepcounter{Table}\par\medskip
\noindent\textbf{TABLE~\thechapter.\theTable: #1 \label{table:\thechapter_\theTable}} \rmfamily}{\medskip}

\newenvironment{Solution}[1][]{\refstepcounter{Solution}\par\medskip
\noindent\textnormal{SOLUTION:} \rmfamily}{\medskip}

\newenvironment{Theorem}[1][]{\refstepcounter{Theorem}\par\medskip
\noindent\textbf{THEOREM~\thechapter.\theTheorem: #1 \label{theorem:\thechapter_\theTheorem}} \rmfamily}{\medskip}	


